<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Restaurant" table="RESTAURANT">
    <indexes>
      <index name="FK_ID_ENTREPRENEUR" columns="ID_ENTREPRENEUR"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="ID_RESTAURANT" columns="ID_RESTAURANT"/>
    </unique-constraints>
    <id name="idRestaurant" type="integer" column="ID_RESTAURANT">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="NOM" length="25" nullable="false"/>
    <field name="telephone" type="string" column="TELEPHONE" length="25" nullable="false"/>
    <field name="adresse" type="string" column="ADRESSE" length="50" nullable="false"/>
    <many-to-one field="idEntrepreneur" target-entity="Entrepreneur">
      <join-columns>
        <join-column name="ID_ENTREPRENEUR" referenced-column-name="ID_ENTREPRENEUR"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
