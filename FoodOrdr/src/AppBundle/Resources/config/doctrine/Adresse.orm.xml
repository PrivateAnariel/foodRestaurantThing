<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Adresse" table="ADRESSE">
    <indexes>
      <index name="FK_ID_CLIENT" columns="ID_CLIENT"/>
    </indexes>
    <id name="idAdresse" type="integer" column="ID_ADRESSE">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numero" type="integer" column="NUMERO" nullable="false"/>
    <field name="rue" type="string" column="RUE" length="25" nullable="false"/>
    <field name="ville" type="string" column="VILLE" length="15" nullable="false"/>
    <field name="codePostal" type="string" column="CODE_POSTAL" length="10" nullable="false"/>
    <many-to-one field="idClient" target-entity="Client">
      <join-columns>
        <join-column name="ID_CLIENT" referenced-column-name="ID_CLIENT"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
